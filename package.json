{
	"name": "agric-invest",
	"version": "1.0.0",
	"main": "bin/www",
	"author": "Usman Akinbiyi",
	"license": "MIT",
	"private": true,
	"scripts": {
		"postinstall": "tsc",
		"start": "node bin/www",
		"server": "nodemon bin/www",
		"dev": "concurrently  -k \"yarn tsc -w\" \"yarn run server\"",
		"test": "jest",
		"test:integration": "cypress run",
		"client:start": "cd client; yarn start"
	},
	"dependencies": {
		"@ant-design/icons": "4.2.1",
		"@hapi/joi": "17.1.0",
		"antd": "4.1.2",
		"bcryptjs": "2.4.3",
		"compression": "1.7.4",
		"cors": "2.8.5",
		"date-fns": "1.30.1",
		"dotenv": "8.0.0",
		"express": "4.17.1",
		"express-graphql": "0.9.0",
		"graphql": "14.4.2",
		"http-errors": "1.7.3",
		"jsonwebtoken": "8.5.1",
		"mongodb-memory-server": "6.3.2",
		"mongoose": "5.9.3",
		"morgan": "1.9.1",
		"prettier": "2.0.5"
	},
	"devDependencies": {
		"@bahmutov/add-typescript-to-cypress": "2.1.2",
		"@types/bcryptjs": "2.4.2",
		"@types/compression": "1.0.0",
		"@types/cors": "2.8.5",
		"@types/express": "4.17.0",
		"@types/express-graphql": "0.8.0",
		"@types/graphql": "14.2.3",
		"@types/hapi__joi": "16.0.12",
		"@types/http-errors": "1.6.1",
		"@types/jest": "24.0.17",
		"@types/jsonwebtoken": "8.3.8",
		"@types/mongoose": "5.7.6",
		"@types/morgan": "1.7.36",
		"@types/node": "10.14.13",
		"@types/supertest": "2.0.8",
		"concurrently": "5.1.0",
		"cypress": "3.4.1",
		"husky": "3.0.3",
		"jest": "24.9.0",
		"nodemon": "2.0.2",
		"pretty-quick": "1.11.1",
		"supertest": "4.0.2",
		"ts-jest": "24.0.2",
		"typescript": "3.5.3",
		"webpack": "4.39.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"diagnostics": {
					"pathRegex": "\\.(spec|test)\\.ts$"
				}
			}
		}
	},
	"resolutions": {
		"@types/node": "10.14.13",
		"lodash": ">=4.17.13"
	}
}
